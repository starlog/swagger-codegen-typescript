{
  "name": "swagger-codegen-typescript",
  "version": "1.1.8",
  "description": "Typescript server boilerplate generator. Created around swagger-codegen (https://github.com/swagger-api/swagger-codegen). With some additional tooling added.",
  "main": "codegen.js",
  "type": "CommonJS",
  "scripts": {
    "test": "jest",
    "lint": "eslint *.ts",
    "lint:fix": "eslint --fix *.ts",
    "build": "tsc"
  },
  "repository": "https://github.com/starlog/swagger-codegen-typescript",
  "homepage": "https://github.com/starlog/swagger-codegen-typescript#readme",
  "bin":  "./codegen.js",
    "keywords": [
    "cli","swagger","codegen"
  ],
  "files": [
    "codegen.js",
    "data.js",
    "tools.js",
    "package.json",
    "package-lock.json",
    "README.md",
    "/codegen/swagger-codegen-cli.jar",
    "/codegen/files/.eslintrc.json",
    "/codegen/files/gitignore",
    "/codegen/files/.gitlab-ci.yml",
    "/codegen/files/Dockerfile",
    "/codegen/files/gateway.json",
    "/codegen/files/index.ts",
    "/codegen/files/package.json",
    "/codegen/files/tsconfig.json",
    "/codegen/files/jest.config.js",
    "/codegen/files/utils/writer.ts",
    "/codegen/files/__tests__/Default.test.ts"
  ],
  "author": "Felix Cho",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.4.0",
    "jsonfile": "^6.1.0",
    "latest-version": "^7.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.6.1",
    "package-json": "7.0.0",
    "shelljs": "^0.8.5",
    "shelljs.exec": "^1.1.8",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "typescript": "^4.7.4"
  }
}
